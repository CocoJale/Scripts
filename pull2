local Players=game:GetService("Players")
local localPlayer=Players.LocalPlayer
local function findNearestUpperTorso1WithinRadius(radius)
    local character=localPlayer.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return nil end
    local hrp=character.HumanoidRootPart
    local closestPart=nil
    local shortestDistance=math.huge
    for _,obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and obj.Name=="UpperTorso1" then
            local playerForChar=Players:GetPlayerFromCharacter(obj.Parent)
            if not playerForChar then
                local distance=(obj.Position-hrp.Position).Magnitude
                if distance<shortestDistance and distance<=radius then
                    shortestDistance=distance
                    closestPart=obj
                end
            end
        end
    end
    return closestPart
end
local function teleportLoop()
    while true do
        local targetPart=findNearestUpperTorso1WithinRadius(100)
        if targetPart then
            local character=localPlayer.Character
            local hrp=character and character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame=CFrame.new(targetPart.Position+Vector3.new(0,3,0))
            end
        else
            warn("No 'UpperTorso1' part found within 100 studs.")
        end
        task.wait(1)
    end
end
task.spawn(teleportLoop)
