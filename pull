local L=loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local T1=T1S1=nil
local Tab1,T1S1= L:NewWindow("Pull A Sword"),nil
local T1S1=Tab1:NewSection("Main Game")
local T2S2=Tab1:NewSection("Squid Minigames")

local E=game:GetService("ReplicatedStorage").Remotes.Events

local function cT(n,p)
 local e=false
 T1S1:CreateToggle(n,function(v)
  e=v
  if not v then return end
  task.spawn(function()
   while e do
    local t=E
    for _,x in ipairs(p) do t=t:WaitForChild(x) end
    t:FireServer()
    task.wait(.01)
   end
  end)
 end)
end

cT("Auto Attack",{"Dungeon","OnDungeonClick"})
cT("Auto Click",{"ClickEvent"})

T1S1:CreateToggle("Generate (LazyFish/BossBot)",function(v)
 _G.AutoWinDragon6=v
 if not v then return end
 task.spawn(function()
  while _G.AutoWinDragon6 do
   local ev=E:WaitForChild("WinMegaBossEvent")
   for i=1,10 do ev:FireServer("Dragon6","Chaos") task.wait() end
  end
 end)
end)

local pl=game.Players.LocalPlayer

T2S2:CreateToggle("Join Minigame",function(v)
 if v then task.spawn(function()
  local r=E.SquidGame.RemoteEvent
  while v do r:FireServer("Accepted") task.wait(5) end
 end) end
end)

T2S2:CreateButton("Win GlassBridge",function()
 local c=pl.Character or pl.CharacterAdded:Wait()
 local r=c:WaitForChild("HumanoidRootPart",2)
 local t=workspace.MiniGameFolder.GlassBridge.End.Wall
 if r and t then r.CFrame=t.CFrame+Vector3.new(0,5,0) end
end)

T2S2:CreateButton("Win RedLight",function()
 local c=pl.Character or pl.CharacterAdded:Wait()
 local r=c:WaitForChild("HumanoidRootPart",2)
 local t=workspace.MiniGameFolder.GreenLight.WinHitbox
 if r and t then r.CFrame=t.CFrame+Vector3.new(0,5,0) end
end)
