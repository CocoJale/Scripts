local p=game:GetService("Players").LocalPlayer
local w=game:GetService("Workspace")
local c=p.Character or p.CharacterAdded:Wait()
local function r(m)return m:FindFirstChild("HumanoidRootPart")or m:FindFirstChildWhichIsA("BasePart")end
local function t(n)
    local hrp=r(c) if not hrp then return end
    for _,m in ipairs(w:GetDescendants())do
        if m:IsA("Model")and m.Name:lower():find(n:lower())then
            local tr=r(m)
            if tr then
                local h=c:FindFirstChildOfClass("Humanoid")
                if h and h.SeatPart then h.Sit=false task.wait(0.1)end
                hrp.CFrame=tr.CFrame
                task.wait(1)
            end
        end
    end
end
task.spawn(function()
    t("Pakrahmatmat")t("Rhino Rhino Toasterinoo")t("Ecco CavaIlo Virtuoso")t("Bisonte Giuppitur Giuppitercito")
end)
