local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/liebertsx/Tora-Library/main/src/librarynew", true))()
local tab = library:CreateWindow("Build A Train")
local folder = tab:AddFolder("Money")

local teleportZ = 100000 -- default Z coordinate

folder:AddBox({
    text = "Enter Z Coordinate",
    flag = "z_coordinate",
    value = tostring(teleportZ),
    callback = function(value)
        local num = tonumber(value)
        if num then
            teleportZ = num
            print("Z coordinate updated to", teleportZ)
        else
            print("Invalid number entered")
        end
    end
})

folder:AddButton({
    text = "Generate Money",
    flag = "generate_money_button",
    callback = function()
        local surface = game:GetService("Workspace").Plots.Location.Cart.Surface
        if surface and surface:IsA("BasePart") then
            surface.Position = Vector3.new(-55, 154, teleportZ)
            print("Surface teleported to:", -55, 154, teleportZ)
        else
            warn("Surface not found or is not a BasePart")
        end
    end
})

library:Close()
library:Init()
