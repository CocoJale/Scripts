local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/liebertsx/Tora-Library/main/src/librarynew", true))()
local tab = library:CreateWindow("Your Title")
local folder = tab:AddFolder("Folder")

local teleportY = 154 -- default Y coordinate

folder:AddBox({
    text = "Enter Y Coordinate",
    flag = "y_coordinate",
    value = tostring(teleportY),
    callback = function(value)
        local num = tonumber(value)
        if num then
            teleportY = num
            print("Y coordinate updated to", teleportY)
        else
            print("Invalid number entered")
        end
    end
})

folder:AddButton({
    text = "Generate Money",
    flag = "generate_money_button",
    callback = function()
        local player = game:GetService("Players").LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(-55, teleportY, 100000)
            print("Player teleported to:", -55, teleportY, 100000)
        else
            warn("Player character or HumanoidRootPart not found")
        end
    end
})

library:Init()
